<!-- Modal -->
<div class="modal fade aky-filemanager-modal" id="aky-filemanager-modal-{{ form.vars.id }}" tabindex="-1" role="dialog">
    <div class="modal-dialog modal-dialog-centered modal-xl" role="document">
        <div class="modal-content">
            <div class="modal-header">
                <h5 class="modal-title">{{ 'Gestionnaire de fichiers'|trans }}</h5>
                <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                    <span aria-hidden="true">&times;</span>
                </button>
            </div>
            <div class="modal-body aky-filemanager-container pt-0 pb-0">
                <iframe class="aky-filemanager-iframe"
                        data-src="{{ path('file_index', {'view': form.vars.view ? form.vars.view : null, 'private_space': form.vars.private_space ? form.vars.private_space : null}) }}"
                        data-target="{{ form.vars.id }}"
                        width="100%"
                        frameborder="0"></iframe>
            </div>
            <div class="modal-footer">
                <button type="button" class="btn btn-secondary" data-dismiss="modal">{{ 'Fermer'|trans }}</button>
            </div>
        </div>
    </div>
</div>
